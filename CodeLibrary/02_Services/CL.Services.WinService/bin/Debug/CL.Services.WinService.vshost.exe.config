<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>

    <add key="Log_Interval" value="600000"/>
    
    <add key="Log_LogParsePath" value="cd C:\Program Files (x86)\Log Parser 2.2\"/>
    <add key="Log_Select" value="SELECT TO_LOCALTIME(TO_TIMESTAMP(ADD(TO_STRING(date, 'yyyy-MM-dd '), TO_STRING(time, 'hh:mm:ss')),'yyyy-MM-dd hh:mm:ss')) AS RequestTime"/>
    <add key="Log_FileDirPath" value="C:\"/>
    <add key="Log_TableName" value="IISLog_table"/>

    <!--WHERE sc-status&lt;&gt;200 OR time-taken&gt;200-->
    <add key="Log_Where" value="WHERE sc-status&lt;&gt;200 OR time-taken&gt;200"/>
    <add key="Log_Server" value="127.0.0.1"/>
    <add key="Log_DBConfig" value="database:Log_IIS -username:sa -password:123"/>
  </appSettings>
</configuration>